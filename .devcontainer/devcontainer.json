{
  "name": "Lunara dev environment",
  "remoteUser": "lunara-developer",
  "service": "dev-machine",
  "workspaceFolder": "/home/lunara-developer/",
  "dockerComposeFile": "compose-dev.yml",
  "customizations": {
    "jetbrains": {
      "backend": "RustRover",
      "plugins": [
        "org.mvnsearch.plugins.justPlugin"
      ]
    },
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "skellock.just",
        "tamasfe.even-better-toml"
      ]
    }
  }
}
